
# API Configuration
# Update this to your local machine IP or server address
EXPO_PUBLIC_API_BASE_URL=http://192.168.x.x:8000/api/v1
EXPO_PUBLIC_WS_BASE_URL=ws://192.168.x.x:8000/ws

# Google OAuth Configuration
# Get these from: https://console.cloud.google.com/apis/credentials
# 1. Create OAuth 2.0 Client ID for Web Application (expoClientId)
# 2. Create OAuth 2.0 Client ID for iOS (needs Bundle ID: com.anonymous.medic)
# 3. Create OAuth 2.0 Client ID for Android (needs Package Name + SHA-1)
# 4. Add authorized redirect URI: https://auth.expo.io/@anonymous/medic
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=YOUR_GOOGLE_WEB_CLIENT_ID
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=YOUR_GOOGLE_IOS_CLIENT_ID
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=YOUR_GOOGLE_ANDROID_CLIENT_ID

# App Configuration
APP_ENV=development
APP_NAME=Medicure
APP_VERSION=1.0.0

# Feature Flags
ENABLE_WEBSOCKET=true
ENABLE_ANALYTICS=false

# Debug
DEBUG=true
EXPO_PUBLIC_TEST_MODE=true


# Database
DATABASE_URL=postgresql://postgres:postgres@db:5432/medicure_dev

# Redis
REDIS_URL=redis://redis:6379


# Environment
ENVIRONMENT=development

# Push Notifications (FCM)
FCM_CREDENTIALS_PATH=mcure-111-684f49b19730.json
FCM_PROJECT_ID=mcure-111

# Sample Mapbox Access Token for Location Services but you can find exact one in .env
MAPBOX_ACCESS_TOKEN=pk.abcdefghijklmnopqrstuvwxyz1234567890


# Google Maps API
# GOOGLE_MAPS_API_KEY=your-api-key-here

# Twilio (Virtual Visits)
# TWILIO_ACCOUNT_SID=your-sid
# TWILIO_AUTH_TOKEN=your-token


# WhatsApp Cloud API (Get these from Meta for Developers)
WHATSAPP_PHONE_ID=123456789012345
WHATSAPP_ACCESS_TOKEN=EAAxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WHATSAPP_TEMPLATE_NAME=otp_verification
WHATSAPP_WEBHOOK_VERIFY_TOKEN=medicure_webhook_token_2025

# JWT (Keep existing)
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30